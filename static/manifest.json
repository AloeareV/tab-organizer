{
  "manifest_version": 2,
  "name": "Tab Organizer - BETA",
  "version": "5.0",
  "description": "Manager for quickly finding and organizing all your tabs.",
  "default_locale": "en",
  "applications": {
    "gecko": {
      "id": "{d70497fe-4de0-47ff-ba0e-48de2a5d338b}"
    }
  },
  "icons": {
     "16": "data/icons/icon16.png",
     "32": "data/icons/icon32.png",
     "48": "data/icons/icon48.png",
    "128": "data/icons/icon128.png"
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "options_ui": {
    "page": "options.html",
    "browser_style": true
  },
  "browser_action": {
    "default_icon": {
      "19": "data/icons/icon19.png"
    }
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+E",
        "mac": "Command+Shift+E"
      }
    }
  },
  "permissions": [
    "tabs",
    "storage",
    "sessions",
    "unlimitedStorage"
  ]
}
