{
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "name": "Tab Organizer - BETA",
  "version": "5.0",
  "description": "Manager for quickly finding and organizing all your tabs.",
  "default_locale": "en",
  "icons": {
     "16": "data/icons/icon16.png",
     "32": "data/icons/icon32.png",
     "48": "data/icons/icon48.png",
    "128": "data/icons/icon128.png"
  },
  "background": {
    "page": "main.html"
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": {
      "19": "data/icons/icon19.png"
    }
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+E",
        "mac": "Command+Shift+E"
      }
    }
  },
  "permissions": [
    "chrome://favicon/",
    "tabs",
    "bookmarks",
    "storage",
    "unlimitedStorage",
    "http://*/*",
    "https://*/*"
  ]
}
